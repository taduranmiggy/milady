{
  "name": "milady-backend",
  "version": "1.0.0",
  "description": "Backend API for Milady - Contraceptive pill tracker and menstrual cycle monitoring app",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "setup": "npm install && npm run db:create && npm run migrate"
  },
  "keywords": [
    "contraceptive",
    "pill-tracker",
    "menstrual-cycle",
    "health",
    "women-health",
    "api",
    "nodejs",
    "express",
    "mysql",
    "sequelize"
  ],
  "author": "Milady Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.35.2",
    "mysql2": "^3.6.5",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.11.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "nodemailer": "^6.9.7",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "sequelize-cli": "^6.6.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^3.1.0",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/milady-team/milady-backend.git"
  },
  "bugs": {
    "url": "https://github.com/milady-team/milady-backend/issues"
  },
  "homepage": "https://github.com/milady-team/milady-backend#readme"
}
